{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:dev": "umi build && cd ./dist && touch app.json && echo '{\n \"servers\": [],\n \"routes\": {\n  \"/\": \"index.html\"\n  }\n}' > app.json ",
    "build:qa": "umi build && cd ./dist && touch app.json && echo '{\n \"servers\": [],\n \"routes\": {\n  \"/\": \"index.html\"\n  }\n}' > app.json ",
    "build:prod": "umi build && cd ./dist && touch app.json && echo '{\n \"servers\": [],\n \"routes\": {\n  \"/\": \"index.html\"\n  }\n}' > app.json  ",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.2.0",
    "@ant-design/pro-layout": "6.0.0",
    "@types/file-saver": "^2.0.5",
    "antd": "4.20.0",
    "axios": "^1.1.3",
    "dva-core": "^1.6.0-beta.7",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "prop-types": "15.5.7",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router": "5.0.0",
    "redbox-react": "1.x",
    "redux": "4.x",
    "umi": "^3.5.35",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.35",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
